{
    "name": "cthunline-api",
    "version": "1.8.2",
    "type": "module",
    "scripts": {
        "dev": "cross-env nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"tsx src/index.ts\"",
        "prod": "node index.js",
        "build": "tsc --build --clean tsconfig.build.json && tsc --project tsconfig.build.json",
        "prepare": "husky",
        "drizzle:generate": "npx drizzle-kit generate:pg --config=src/drizzle/config.ts",
        "test": "dotenv -e .env.test -- vitest --run",
        "test:watch": "dotenv -e .env.test -- vitest --watch",
        "lint": "npx @biomejs/biome check && npx tsc --noEmit",
        "format": "npx @biomejs/biome check --write && npx tsc --noEmit"
    },
    "dependencies": {
        "@cthunline/games": "^3.3.2",
        "@fastify/cookie": "^9.3.1",
        "@fastify/helmet": "^11.1.1",
        "@fastify/rate-limit": "^9.1.0",
        "@fastify/static": "^7.0.4",
        "@pitininja/envious": "^3.1.8",
        "@sinclair/typebox": "^0.32.34",
        "ajv-formats": "^3.0.1",
        "bcrypt": "^5.1.1",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.12",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.32.0",
        "fastify": "^4.28.1",
        "fastify-formidable": "^3.0.2",
        "fastify-qs": "^4.0.2",
        "fastify-socket.io": "^5.0.0",
        "formidable": "^3.5.1",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "nanoid": "^5.0.7",
        "pg": "^8.12.0",
        "qs": "^6.12.3",
        "socket.io": "^4.7.5",
        "tsx": "^4.16.2",
        "winston": "^3.13.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.8.3",
        "@tsconfig/recommended": "^1.0.7",
        "@types/bcrypt": "^5.0.2",
        "@types/deep-equal-in-any-order": "^1.0.3",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.14.11",
        "@types/pg": "^8.11.6",
        "@types/qs": "^6.9.15",
        "@types/type-detect": "^4.0.3",
        "@vitest/coverage-istanbul": "^2.0.3",
        "deep-equal-in-any-order": "^2.0.6",
        "dotenv-cli": "^7.4.2",
        "drizzle-kit": "^0.23.0",
        "form-data": "^4.0.0",
        "husky": "^9.1.1",
        "mockdate": "^3.0.5",
        "nodemon": "^3.1.4",
        "socket.io-client": "^4.7.5",
        "type-detect": "^4.0.8",
        "typescript": "^5.5.3",
        "vitest": "^2.0.3"
    }
}
