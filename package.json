{
    "name": "cthunline-api",
    "version": "1.14.0",
    "type": "module",
    "scripts": {
        "dev": "cross-env nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"tsx src/index.ts\"",
        "prod": "node index.js",
        "build": "tsc --build --clean tsconfig.build.json && tsc --project tsconfig.build.json",
        "prepare": "[ -d '.husky' ] && husky || true",
        "drizzle:up": "npx drizzle-kit up --config=src/drizzle/config.ts",
        "drizzle:generate": "npm run drizzle:up && npx drizzle-kit generate --config=src/drizzle/config.ts",
        "drizzle:push": "npx drizzle-kit push --config=src/drizzle/config.ts",
        "drizzle:migrate": "npx drizzle-kit migrate --config=src/drizzle/config.ts",
        "pretest": "dotenv -e .env.test -- npx drizzle-kit push --force --config=src/drizzle/config.ts",
        "test": "dotenv -e .env.test -- vitest --run",
        "test:watch": "dotenv -e .env.test -- vitest --watch",
        "lint": "npx @biomejs/biome check --error-on-warnings && npx tsc --noEmit",
        "format": "npx @biomejs/biome check --write --error-on-warnings && npx tsc --noEmit"
    },
    "dependencies": {
        "@cthunline/games": "^3.4.2",
        "@fastify/cookie": "^9.4.0",
        "@fastify/helmet": "^11.1.1",
        "@fastify/rate-limit": "^9.1.0",
        "@fastify/static": "^7.0.4",
        "@pitininja/envious": "^3.3.0",
        "@sinclair/typebox": "^0.33.9",
        "ajv-formats": "^3.0.1",
        "bcrypt": "^5.1.1",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.13",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.33.0",
        "fastify": "^4.28.1",
        "fastify-formidable": "^3.0.2",
        "fastify-qs": "^4.0.2",
        "fastify-socket.io": "^5.1.0",
        "formidable": "^3.5.1",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "ms": "^2.1.3",
        "nanoid": "^5.0.7",
        "pg": "^8.12.0",
        "qs": "^6.13.0",
        "socket.io": "^4.7.5",
        "tsx": "^4.19.0",
        "winston": "^3.14.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.8.3",
        "@tsconfig/recommended": "^1.0.7",
        "@types/bcrypt": "^5.0.2",
        "@types/deep-equal-in-any-order": "^1.0.3",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/ms": "^0.7.34",
        "@types/node": "^22.5.3",
        "@types/pg": "^8.11.8",
        "@types/qs": "^6.9.15",
        "@types/type-detect": "^4.0.3",
        "@vitest/coverage-istanbul": "^2.0.5",
        "deep-equal-in-any-order": "^2.0.6",
        "dotenv-cli": "^7.4.2",
        "drizzle-kit": "^0.24.2",
        "form-data": "^4.0.0",
        "husky": "^9.1.5",
        "mockdate": "^3.0.5",
        "nodemon": "^3.1.4",
        "socket.io-client": "^4.7.5",
        "type-detect": "^4.1.0",
        "typescript": "^5.5.4",
        "vitest": "^2.0.5"
    }
}
