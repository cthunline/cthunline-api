{
    "DEFAULT_ADMIN_NAME": {
        "type": "string",
        "required": true
    },
    "DEFAULT_ADMIN_EMAIL": {
        "type": "string",
        "required": true
    },
    "DEFAULT_ADMIN_PASSWORD": {
        "type": "string",
        "required": true
    },
    "REGISTRATION_ENABLED": {
        "type": "boolean",
        "required": true
    },
    "INVITATION_ENABLED": {
        "type": "boolean",
        "required": false
    },
    "DEFAULT_THEME": {
        "type": "string",
        "required": true,
        "filter": ["dark", "light"]
    },
    "DEFAULT_LOCALE": {
        "type": "string",
        "required": true,
        "filter": ["fr", "en"]
    },
    "ENVIRONMENT": {
        "type": "string",
        "required": true,
        "filter": ["dev", "prod"]
    },
    "PORT": {
        "type": "number",
        "required": true
    },
    "JWT_SECRET": {
        "type": "string",
        "required": true
    },
    "COOKIE_SECRET": {
        "type": "string",
        "required": true
    },
    "COOKIE_SECURE": {
        "type": "boolean",
        "required": true
    },
    "LOG_ENABLED": {
        "type": "boolean",
        "required": true
    },
    "LOG_DIR": {
        "type": "string",
        "required": false
    },
    "REVERSE_PROXY": {
        "type": "boolean",
        "required": true
    },
    "RL_WINDOW_DURATION": {
        "type": "number",
        "required": true
    },
    "RL_MAX_REQUESTS": {
        "type": "number",
        "required": true
    },
    "DATABASE_URL": {
        "type": "string",
        "required": true
    },
    "ASSET_DIR": {
        "type": "string",
        "required": true
    },
    "ASSET_MAX_SIZE_MB": {
        "type": "number",
        "required": true
    },
    "ASSET_MAX_SIZE_MB_PER_FILE": {
        "type": "number",
        "required": true
    }
}
